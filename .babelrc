{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "edge": "17",
          "firefox": "60",
          "chrome": "67",
          "safari": "11.1"
        },
        "useBuiltIns": "usage",
        "shippedProposals": true,
        "corejs": { "version": 3 }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": "./src"
      }
    ],
    "@babel/plugin-transform-arrow-functions"
  ]
}
